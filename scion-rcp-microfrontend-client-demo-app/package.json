{
  "name": "scion-rcp-microfrontend-client-demo-app",
  "scripts": {
    "start": "cpy src/*.json dist && parcel src/*.html --port 4201 --no-cache --open"
  },
  "dependencies": {
    "@scion/microfrontend-platform": "1.2.2",
    "@scion/toolkit": "1.4.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.15.4",
    "@types/node": "24.2.1",
    "cpy-cli": "6.0.0",
    "parcel": "2.15.4",
    "typescript": "5.9.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "alias": {
    "process": false,
    "@scion/toolkit/*": "./node_modules/@scion/toolkit/fesm2022/scion-toolkit-$1.mjs"
  }
}
