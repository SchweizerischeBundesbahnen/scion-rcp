    "@scion/toolkit": "1.3.1",
    "rxjs": "7.5.7"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "@types/node": "^16.0.0",
    "cpy-cli": "^4.2.0",
    "parcel": "^2.7.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "alias": {
