{
  "name": "scion-rcp-microfrontend-host-dependency-bundler",
  "scripts": {
    "bundle": "parcel build src/refs.ts --no-cache --dist-dir ../ch.sbb.scion.rcp.microfrontend/js"
  },
  "dependencies": {
    "@scion/microfrontend-platform": "1.2.2",
    "@scion/toolkit": "1.4.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/node": "24.2.1",
    "parcel": "2.15.4",
    "typescript": "5.9.2"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "alias": {
    "process": false,
    "@scion/toolkit/*": "./node_modules/@scion/toolkit/fesm2022/scion-toolkit-$1.mjs"
  }
}
